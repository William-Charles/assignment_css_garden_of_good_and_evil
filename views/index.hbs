<div class="container">
<h1>CSS Garden of Good and Evil</h1>
<br>
<br>
<br>
<div class="properties-form-container">
<form class="properties-form" action="/" method="post">
  <h3>Properties</h3>
  <label for="good">Good</label>
    <input type="radio" name="goodEvil" value="good">
  <label for="evil">Evil</label>
    <input type="radio" name="goodEvil" value="evil">
  <br><br>
  <label for="fav-food">Favorite Food</label>
    <input type="text" name="fav-food">
    
  <label for="color">Favorite Color</label>
    <select class="" name="color">
    <option value="red">red</option>
    <option value="blue">blue</option>
    <option value="green">green</option>
  </select>
  
  <label for="insanity">Insanity Level</label>
    <input type="range" name="insanity" min="1"
  max="10" step="1">
  <br><br>
    <input type="submit">
</form>
</div>
<h2>Favorite Things</h2>
<ul>
{{#each personaTrait as |trait|}}
  <li>{{trait}}</li>
{{/each}}
</ul>
</div>